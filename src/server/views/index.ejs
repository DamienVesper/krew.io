        <!-- Bank Modal -->
        <div class="bank-modal modal">
            <div class="modal-header">
                <h6 class="modal-title">Welcome to the Bank at Labrador!</h6>
                <div class="close-bank-btn btn btn-secondary btn-sm float-right">
                    <i class="icofont icofont-ui-close"></i>
                </div>
            </div>
            <div class="modal-content">
                <div class="container bank-loggedin" style="display: none;">
                    <span class="deposit-text">Your gold in the bank: <span class="deposit-amt"></span>.</span>

                    <p class="how-to-use-bank">
                        The bank accepts up to 50,000 gold to save permanently, and you can use that amount the next time you login.
                        You can additionally store another 100,000 within the bank, but it will be lost as soon as you leave the game.
                        Each withdrawal incurs a transaction fee of 10% the amount that you withdraw.
                    </p>

                    <div class="bank-error text-danger"></div>
                    <div class="bank-success text-success"></div>

                    <div class="deposit-area">
                        <div class="form-group">
                            <select name="transaction-type" id="transaction-type" class="custom-select form-control">
                                <option value="deposit">Deposit</option>
                                <option value="withdraw">Withdraw</option>
                            </select>
                        </div>
                        <div class="form-group">
                            <input type="text" name="username-input" id="username-input" maxlength="32">
                            <input type="number" name="transaction-amt" id="transaction-amt" max="125000" min="1">
                        </div>
                    </div>
                </div>
                <div class="container bank-notloggedin" style="display: none;">
                    <span class="text-warning">To use the bank, please log in to your account.</span>
                </div>
            </div>
        </div>

